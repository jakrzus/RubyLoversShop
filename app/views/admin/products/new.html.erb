<div class="col-6">
  <%= form_for product, url: admin_products_path, method: :post do |f| %>
  <div class="form-group">
    <%= f.label :name %><br/>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :price %><br/>
    <%= f.number_field :price, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :brand_id %><br/>
    <%= f.select :brand_id, BrandPresenter.select_params, class: "form-control" , include_blank: true %>
  </div>
  <div class="form-group">
    <%= f.label :category_id %><br/>
    <%= f.select :category_id, CategoryPresenter.select_params, class: "form-control" , include_blank: true%>
  </div>
  <%= f.submit "Submit" , class: "btn btn-primary" %>
  <% end %>
</div>
